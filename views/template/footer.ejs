</script>
<script>
    //Modal toggle 
    $(function () {
        if ($.cookie("vcartAuth")) {
            let token = JSON.parse($.cookie("vcartAuth"));
            if (token.is_email_verified == false) {
                location.href = "/otp"
            }
            $.ajax({
                type: 'GET',
                contentType: 'application/json',
                url: '<%= process.env.APIURL %>/cart/listV2',
                beforeSend: function (xhr) {
                    xhr.setRequestHeader('Authorization', 'Bearer ' + token.token);
                }
            }).done(function (resp) {
                if (resp.results.status == "200") {
                    if (resp.results.response.pendingBundle.length > 0) {
                        $("#bundleicon").removeClass("hide");
                        localStorage.bundleCount = (resp.results.response.pendingBundle[0].c > 0) ?
                            resp.results.response.pendingBundle[0].c : 0;
                        var cartCount = (resp.results.response.pendingBundle[0].c > 0) ? resp.results.response
                            .pendingBundle[0].c : 0;
                        $("#bundleicon a").attr("href", "/mybundles/view/" + resp.results.response.pendingBundle[
                            0].ub_id)
                        $(".cart-label").text(cartCount);
                    } else {
                        $("#carticon").removeClass("hide");
                        var cartList = resp.results.response.cartItems;
                        var cartCount = (typeof cartList != "undefined") ? cartList.length : 0;
                        localStorage.cartCount = (cartCount > 0) ? cartCount : 0;
                        $(".cart-label").text(cartCount);
                    }
                }
            }).fail(function (r) {
                console.log(JSON.stringify(r));
                toastr.error(r.responseJSON.error);
            });
        }

        $(".refererLogin").on("click", function () {
            var referrer_uri = $(this).data("referrer");
            $(".referrerInput").val(referrer_uri);
        });

        $('#register-form-link').click(function (e) {
            $("#register-form").delay(100).fadeIn(100);
            $("#login-form").fadeOut(100);
            $('#login-form-link').removeClass('active');
            $(this).addClass('active');
            e.preventDefault();
        });

        $('#login-form-link').click(function (e) {
            $("#login-form").delay(100).fadeIn(100);
            $("#register-form").fadeOut(100);
            $('#register-form-link').removeClass('active');
            $(this).addClass('active');
            e.preventDefault();
        });
        // var note=localStorage.getItem('notified');
        // if(!note){
        //     localStorage.setItem('notified',1);
        // swal({
        //     title: 'delivery will start from 12th October!',
